<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingTop="@dimen/activity_margin"
    android:paddingBottom="@dimen/activity_margin"
    android:paddingLeft="30dp"
    android:paddingRight="30dp"
    android:gravity="center_horizontal"
    android:background="@color/white"
    android:focusable="true"
    android:clickable="true"
    tools:context=".tendero.Inicio_sesion.ui.Mis_productos.mip_frgmenuproductos">

    <Button
        android:id="@+id/mip_mpbtnregistrarproducto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/mit_varstrmenuproductos1"
        android:layout_marginBottom="20dp"
        android:drawableLeft="@drawable/mit_iconregistrar"
        style="@style/Outlined_buttonXL_black"/>
    <Button
        android:id="@+id/mip_mpbtnreporteproductos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/mit_varstrmenuproductos2"
        android:layout_marginBottom="20dp"
        android:drawableLeft="@drawable/mit_iconreporte"
        style="@style/Outlined_buttonXL_black"/>

</LinearLayout>